
public class CircularBlob extends Blob {
	
	private double radius=5;
	private double angle=0;
//	private double startX,startY;
	
	public CircularBlob(double a ,double b){
		super(a,b);
//		this.startX=startX;
//		this.startY=startY;
		
	}
	
	public void step() {
		x= radius*Math.cos(angle);
		y= radius*Math.cos(angle);
		
		angle+=1;
	}
	
}
